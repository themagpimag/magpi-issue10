            <ITEXT FONT="Monaco Regular" CH="#include &lt;iostream&gt;"/>
            <para/>
            <ITEXT FONT="Monaco Regular" CH="#include &lt;fstream&gt;"/>
            <para/>
            <ITEXT FONT="Monaco Regular" CH="#include &lt;string&gt;"/>
            <para/>
            <ITEXT FONT="Monaco Regular" CH="using namespace std;"/>
            <para/>
            <para/>
            <ITEXT FONT="Monaco Regular" CH="int main()"/>
            <para/>
            <ITEXT FONT="Monaco Regular" CH="{"/>
            <para/>
            <ITEXT FONT="Monaco Regular" CH="  // Create an input file stream named 'reader':"/>
            <para/>
            <ITEXT FONT="Monaco Regular" CH="  ifstream reader(“test.txt”);"/>
            <para/>
            <ITEXT FONT="Monaco Regular" CH="  while(reader.good())"/>
            <para/>
            <ITEXT FONT="Monaco Regular" CH="  {"/>
            <para/>
            <ITEXT FONT="Monaco Regular" CH="    string temp;"/>
            <para/>
            <ITEXT FONT="Monaco Regular" CH="    getline(reader, temp);"/>
            <para/>
            <ITEXT FONT="Monaco Regular" CH="    cout &lt;&lt; temp &lt;&lt; endl;"/>
            <para/>
            <ITEXT FONT="Monaco Regular" CH="  }"/>
            <para/>
            <ITEXT FONT="Monaco Regular" CH="  reader.close();"/>
            <para/>
            <ITEXT FONT="Monaco Regular" CH="  return 0;"/>
            <para/>
            <ITEXT FONT="Monaco Regular" CH="}"/>
            <trail/>
